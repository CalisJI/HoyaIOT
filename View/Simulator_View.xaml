        <Grid Grid.Row="2">
            <Grid.ColumnDefinitions>
                <ColumnDefinition/>
                <ColumnDefinition/>
            </Grid.ColumnDefinitions>
            <Border Grid.Column="0" Background="#FF69A08F" CornerRadius="30" Margin="15">
                <StackPanel Orientation="Vertical">
                    <Label
                        Content="TimeLine"
                        HorizontalAlignment="Center"
                        FontSize="23"
                        FontWeight="DemiBold"/>
                    <ListView
                        DataContext="{StaticResource RuntimeData}"
                        ItemsSource="{Binding BatchTrackingDatas}"
                        Background="AliceBlue"
                        ScrollViewer.VerticalScrollBarVisibility="Auto"
                        ScrollViewer.HorizontalScrollBarVisibility="Auto"
                        HorizontalAlignment="Center"
                        Width="800"
                        Height="530">
                        <ListView.View>
                            <GridView>

                                <GridViewColumn>
                                    <GridViewColumn.Header>
                                        <TextBlock Text="STT" FontWeight="DemiBold"/>
                                    </GridViewColumn.Header>
                                    <GridViewColumn.CellTemplate>
                                        <!--{Binding RelativeSource={RelativeSource Mode=TemplatedParent}, Path=(ListView.AlternationIndex)}-->
                                        <DataTemplate>
                                            <TextBlock Text="{Binding STT}"/>
                                        </DataTemplate>
                                    </GridViewColumn.CellTemplate>
                                </GridViewColumn>

                                <GridViewColumn>
                                    <GridViewColumn.Header>
                                        <TextBlock Text="Barcode" FontWeight="DemiBold"/>
                                    </GridViewColumn.Header>
                                    <GridViewColumn.CellTemplate>
                                        <DataTemplate>
                                            <TextBlock Text="{Binding QRCode}"/>
                                        </DataTemplate>
                                    </GridViewColumn.CellTemplate>
                                </GridViewColumn>

                                <GridViewColumn>
                                    <GridViewColumn.Header>
                                        <TextBlock Text="Vị trí" FontWeight="DemiBold"/>
                                    </GridViewColumn.Header>
                                    <GridViewColumn.CellTemplate>
                                        <DataTemplate>
                                            <TextBlock Text="{Binding Location, Converter={StaticResource Loc}}"/>
                                        </DataTemplate>
                                    </GridViewColumn.CellTemplate>
                                </GridViewColumn>

                                <GridViewColumn>
                                    <GridViewColumn.Header>
                                        <TextBlock Text="Thời gian vào" FontWeight="DemiBold"/>
                                    </GridViewColumn.Header>
                                    <GridViewColumn.CellTemplate>
                                        <DataTemplate>
                                            <TextBlock Text="{Binding TimeIn, StringFormat='{}{0: HH:mm:ss}'}"/>
                                        </DataTemplate>
                                    </GridViewColumn.CellTemplate>
                                </GridViewColumn>

                                <GridViewColumn>
                                    <GridViewColumn.Header>
                                        <TextBlock Text="Đánh giá"/>
                                    </GridViewColumn.Header>
                                    <GridViewColumn.CellTemplate>
                                        <DataTemplate>
                                            <materialDesign:PackIcon Kind="Check"/>
                                        </DataTemplate>
                                    </GridViewColumn.CellTemplate>
                                </GridViewColumn>
                            </GridView>
                        </ListView.View>
                    </ListView>
                </StackPanel>

            </Border>
            <StackPanel Orientation="Vertical" Grid.Column="1">
                <TextBox 
                    x:Name="BarCode"
                    HorizontalAlignment="Left"
                    Margin="50,20,0,0"
                    FontSize="22" 
                    FontWeight="DemiBold"
                    Width="250"/>
                <Button
                    Margin="50,10,0,0"
                    HorizontalAlignment="Left"
                    Width="150"
                    DataContext="{StaticResource RuntimeData}"
                    Command="{Binding Scannew}"
                    CommandParameter="{Binding ElementName=BarCode, Path=Text}"
                    Content="Demo Scan"/>
                <Button
                    Margin="50,10,0,0"
                    HorizontalAlignment="Left"
                    Width="150"
                    DataContext="{StaticResource RuntimeData}"
                    Command="{Binding Trigger}"
                    CommandParameter="{Binding ElementName=BarCode, Path=Text}"
                    Content="Demo Process"/>
                <Button
                    Margin="50,10,0,0"
                    HorizontalAlignment="Left"
                    Width="150"
                    DataContext="{StaticResource RuntimeData}"
                    Command="{Binding Runsim}"
                    Content="Run Simulator"/>
                <CheckBox
                    DataContext="{StaticResource RuntimeData}"
                    IsChecked="{Binding WaitingInput}"
                    Content="Test B1"/>


            </StackPanel>
        </Grid>
